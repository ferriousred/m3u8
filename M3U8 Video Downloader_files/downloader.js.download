(()=>{let browserType;let webStore;if(window.navigator.userAgent.indexOf("Edg")>-1){browserType='edgeInstall';webStore='https://microsoftedge.microsoft.com/addons/detail/dbepbhhcmhodojepbagfppgpieeplpik';}else{browserType='chromeInstall';webStore='https://chromewebstore.google.com/detail/nfmmmhanepmpifddlkkmihkalkoekpfd';}
document.getElementById(browserType).classList.remove('d-none');setTimeout(()=>{const $UILoading=document.getElementById('UILoading');const $UIInstall=document.getElementById('UIInstall');const $oldDownloader=document.getElementById('oldDownloader');if($UILoading){$UILoading.classList.add('d-none');if($oldDownloader)$oldDownloader.classList.remove('d-none');}},3000);document.querySelectorAll('.faq-collapse').forEach($collapse=>{const $header=$collapse.querySelector('.faq-header');const $dash=$header.querySelector('.collapse-dash');const $plus=$header.querySelector('.collapse-plus');const $body=$collapse.querySelector('.faq-body');const collapse=new bootstrap.Collapse($body,{toggle:false,});$body.addEventListener('hidden.bs.collapse',event=>{$dash.classList.add('d-none');$plus.classList.remove('d-none');});$body.addEventListener('shown.bs.collapse',event=>{$dash.classList.remove('d-none');$plus.classList.add('d-none');});$header.onclick=()=>collapse.toggle();});const $review=document.getElementById('review');const $reviewBtn=document.getElementById('reviewBtn');const review=new bootstrap.Toast($review);$review.addEventListener('hidden.bs.toast',()=>{if(!localStorage.getItem('reviewed'))localStorage.setItem('reviewed',1);});$reviewBtn.onclick=()=>{window.open(webStore);review.hide();};setTimeout(()=>{const $save=document.querySelector('[selector="save"]');if(!$save)return;if(localStorage.getItem('reviewed'))return;$save.addEventListener('click',()=>{let size=$save.dataset.size;if(!size)return;size=parseInt(size);if(size>500*1024*1024&&size<1.5*1024*1024*1024){setTimeout(()=>{review.show();},5000);}});},10000);})()